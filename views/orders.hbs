<h3>My Orders</h3>
{{#if orders.length}}
  {{#each orders}}
    <div class="Yorder">
      <table>
        <tr>
          <th colspan="2">Order Id: </th>
          <th colspan="2"><small>{{_id}}</small></th>
        </tr>
        <th colspan="2">Name:
          {{user.userId.name}}, email:
          <i>{{user.userId.email}}</i>
        </th>
        <tr>
        </tr>
        {{#each notebooks}}
          <tr>
            <td>{{notebook.title}} x {{count}}</td>
            <td>$88.00</td>
          </tr>
        {{/each}}
      </table>
      <p>Price: <span class="price">{{notebook.price}}</span></p>
      <button type="button" class="btn">Place Order</button>
    </div>
  {{/each}}
{{else}}
  <p>Not found</p>
{{/if}}