<h3>My Orders</h3>
{{#if orders.length}}
  {{#each orders}}
    <div class="card">
      <div class="card-header">
        <p><i>{{user.userId.name}} </i>{{user.userId.email}}</p>
      </div>
      <div class="card-content">
        <ol>
          {{#each notebooks}}
            <li>
              {{notebook.title}}
              (x
              <b>{{count}}</b>)
            </li>
          {{/each}}
        </ol>
      </div>
      <div class="card-action actions">
        <p class="date">{{user.date}}</p>
        <p>Price: <span class="price">{{price}}</span></p>
      </div>
    </div>
  {{/each}}
{{else}}
  <p>Not found</p>
{{/if}}